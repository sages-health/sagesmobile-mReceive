[
	{
		"pk": 1, 
		"model": "rapidandroid.fieldtype", 
		"fields": {
			"datatype": "word", 
			"regex": "^(([A-Za-z]+)|(\\.))($|\\s)", 
			"name": "Word"
			}
	}, 
	{
		"pk": 2, 
		"model": "rapidandroid.fieldtype", 
		"fields": {
			"datatype": "number", 
			"regex": "^(([-]?\\d+\\.?\\d*)|(\\.))($|\\s)",
			"name": "Number"
		}
	}, 
	{
		"pk": 3, 
		"model": "rapidandroid.fieldtype", 
		"fields": {
			"datatype": "float", 
			"regex": "^((\\d+)|(\\d+\\.\\d+))(\\s*(kg|kilo|kilos))($|\\s)", 
			"name": "Weight"
		}
	}, 
	{"pk": 4, 
		"model": "rapidandroid.fieldtype", 
		"fields": {
